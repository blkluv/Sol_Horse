{
  "name": "sol-horse",
  "private": true,
  "version": "1.0.0",
  "description": "AI-Powered Solana Horse Racing Game - A blockchain-based horse racing game with NFT ownership, breeding, and competitive racing on the Solana network.",
  "type": "module",
  "keywords": [
    "solana",
    "nft",
    "horse-racing",
    "blockchain",
    "gaming",
    "react",
    "typescript",
    "web3"
  ],
  "author": "ereezyy",
  "repository": {
    "type": "git",
    "url": "https://github.com/ereezyy/Sol_Horse.git"
  },
  "homepage": "https://github.com/ereezyy/Sol_Horse#readme",
  "bugs": {
    "url": "https://github.com/ereezyy/Sol_Horse/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.19.4",
    "@solana/spl-token": "^0.3.8",
    "lucide-react": "^0.263.1",
    "@supabase/supabase-js": "^2.39.0"
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.22",
    "@solana/web3.js": "^1.78.4",
    "@supabase/supabase-js": "^2.38.4",
    "@solana/web3.js": "^1.87.6",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test --run && npm run build"
    }
  }
}

